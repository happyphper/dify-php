{
  "name": "happyphper/dify",
  "description": "Dify PHP SDK",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Baron",
      "email": "baronbool92@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "psr/log": "^1.0|^2.0|^3.0",
    "hyperf/guzzle": "^3.1",
    "ext-fileinfo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "hyperf/framework": "^3.0",
    "hyperf/config": "^3.0",
    "hyperf/logger": "^3.0",
    "phpunit/php-code-coverage": "^9.0"
  },
  "suggest": {
    "hyperf/framework": "^3.0",
    "hyperf/config": "^3.0",
    "hyperf/logger": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Happyphper\\Dify\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Happyphper\\Dify\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit tests/Cases/",
    "test-coverage": "phpunit tests/Cases/ --coverage-html coverage"
  },
  "extra": {
    "hyperf": {
      "config": "Happyphper\\Dify\\ConfigProvider"
    }
  }
}
