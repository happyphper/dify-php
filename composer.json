{
    "name": "happyphper/dify",
    "description": "Dify PHP SDK",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Baron",
            "email": "baronbool92@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.0",
        "psr/log": "^1.0|^2.0|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "hyperf/framework": "^3.0",
        "hyperf/config": "^3.0",
        "hyperf/logger": "^3.0"
    },
    "suggest": {
        "hyperf/framework": "^3.0",
        "hyperf/config": "^3.0",
        "hyperf/logger": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Happyphper\\Dify\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Happyphper\\Dify\\Tests\\": "tests/"
        }
    },
    "extra": {
        "hyperf": {
            "config": "Happyphper\\Dify\\ConfigProvider"
        }
    }
} 